    ".npm*/**": true,
    ".nvm*/**": true,
    ".bower*/**": true,
    ".ipython": true,
    ".local*/**": true,
    ".oh-my-zsh*/**": true,
    ".cache": true,
    "miniconda3": true,
    ".condarc": true,
    ".gitconfig": true,
    ".hushlogin": true,
    ".profile": true,
    ".screenrc": true,
    ".sudo_as_admin_successful": true,
    ".zcompdump*": true,
    ".bash_history": true,
    ".bashrc": true,
    ".zsh_history": true,
    ".zshrc": true,
    ".zshenv": true,
